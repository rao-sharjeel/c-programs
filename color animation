#include<stdio.h>
#include <time.h>
#include <stdlib.h>
void delay(float number_of_seconds) 
{ 
    // Converting time into milli_seconds 
    float milli_seconds = 1000 * number_of_seconds; 
  
    // Storing start time 
    clock_t start_time = clock(); 
  
    // looping till required time is not achieved 
    while (clock() < start_time + milli_seconds) 
        ; 
} 
int main()
{
	delay(0.5);
	printf("\n\n\t\t the logoooo ");
	system("COLOR F0");
	delay(0.5);
	system("COLOR D6");
	delay(0.5);
	system("COLOR 4B");
	delay(0.5);
	system("COLOR 1F");
	delay(0.5);
	system("COLOR D7");
	delay(0.5);
	system("COLOR 3F");
	delay(0.5);
	system("COLOR 8A");
	delay(0.5);
	system("COLOR 4B");
	delay(0.5);
	system("COLOR 1F");
	delay(0.5);
	system("COLOR F3");
	delay(0.5);
	system("COLOR E4");
	delay(0.5);
	system("COLOR A7");
	delay(0.5);
	system("COLOR 0F");

    return 0; 
	
}
