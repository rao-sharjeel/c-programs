#include<stdio.h>
//-----------------Prime number checker by RAO SHARJEEL-----
int
main()
{
    int num, is_prime;
    printf("enter a number to check if it is prime or not ");
    scanf("%d", &num);
    is_prime=1;
    for(int i = 2; i<=num/2 ; i++)
    {
        if(num%i==0)
            is_prime=0;
    }
    if(is_prime==0)
        printf("it is not prime");
    else
        printf("it is prime");
        return(0);
}
