#include <stdio.h>

int main()
{
    int num, r, sum=0, a, temp;
    
    printf("Enter a number to check Amstrong number ");
    scanf("%d", &num);
    temp = num;
    while(num>0)
    {
        r = num%10;
        sum = sum + (r*r*r);
        num = num/10;
    }
    if(sum == temp)
    {
        printf("it is an armstrong number ");
    }
    else{
        printf("it is not an armstrong number ");
    }
    
    //printf("%d", sum);
    
    return 0;
}
